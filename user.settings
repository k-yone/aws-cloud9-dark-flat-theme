{
    "ace": {
        "@keyboardmode": "default",
        "@showGutter": true,
        "@theme": "ace/theme/cloud9_night_low_color",
        "statusbar": {
            "@show": true
        }
    },
    "build": {
        "@autobuild": false
    },
    "collab": {
        "@timeslider-visible": false
    },
    "debug": {
        "@autoshow": true,
        "@pause": 0
    },
    "general": {
        "@skin": "flat-dark"
    },
    "openfiles": {
        "@show": false
    },
    "output": {
        "@backgroundColor": "#003a58",
        "@foregroundColor": "#FFFFFF",
        "@keepOutput": false,
        "@nosavequestion": false,
        "@selectionColor": "#225477"
    },
    "projecttree": {
        "@showhidden": false
    },
    "tabs": {
        "@show": true
    },
    "terminal": {
        "@antialiasedfonts": false,
        "@backgroundColor": "#153649",
        "@defaultEnvEditor": true,
        "@foregroundColor": "#FFFFFF",
        "@selectionColor": "#515D77"
    }
}
